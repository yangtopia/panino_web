{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@containers": "./src/containers",
          "@components": "./src/components",
          "@store": "./src/store",
          "@static": "./src/static",
          "@utils": "./src/utils",
          "@models": "./src/models",
          "@hooks": "./src/hooks",
          "@styles": "./src/styles"
        },
        "extensions": [".js", ".jsx", ".tsx"]
      }
    ]
  ]
}
