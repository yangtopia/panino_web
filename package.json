{
  "name": "panino",
  "version": "1.0.0",
  "description": "panino web project",
  "author": "yangtopia@gmail.com",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next",
    "preserve": "npm run build-public && npm run build-functions && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-app": "next build",
    "build-public": "cpx \"public/**/*.*\" \"dist/public\" -C",
    "build-functions": "tsc --project tsconfig.functions.json",
    "lint-app": "eslint \"src/**/*.{js,ts,tsx}\"",
    "typecheck-app": "tsc --project tsconfig.json",
    "lint-functions": "eslint \"functions/**/*.{js,ts,tsx}\"",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.0",
    "moment": "^2.24.0",
    "next": "latest",
    "next-redux-wrapper": "^5.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-localize-redux": "^3.5.3",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.0",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/styled-components": "^5.1.0",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cpx": "1.5.0",
    "cross-env": "7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-module-resolver": "^0.16.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "firebase-tools": "^8.0.2",
    "prettier": "^2.0.4",
    "rimraf": "3.0.2",
    "typescript": "^3.8.3"
  }
}
